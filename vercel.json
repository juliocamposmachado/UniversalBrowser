{
  "version": 2,
  "name": "universal-browser",
  "builds": [
    {
      "src": "Website/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/Website/index.html"
    },
    {
      "src": "/documentation",
      "dest": "/Website/documentation.html"
    },
    {
      "src": "/changelog", 
      "dest": "/Website/changelog.html"
    },
    {
      "src": "/downloads/(.*)",
      "dest": "/Website/downloads/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/Website/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Website/$1"
    }
  ],
  "headers": [
    {
      "source": "/downloads/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/octet-stream"
        },
        {
          "key": "Content-Disposition",
          "value": "attachment"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}