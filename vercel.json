{
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/",
      "destination": "/Website/index.html"
    },
    {
      "source": "/documentation",
      "destination": "/Website/documentation.html"
    },
    {
      "source": "/changelog", 
      "destination": "/Website/changelog.html"
    },
    {
      "source": "/downloads/(.*)",
      "destination": "/Website/downloads/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/Website/$1"
    }
  ],
  "headers": [
    {
      "source": "/downloads/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/octet-stream"
        },
        {
          "key": "Content-Disposition",
          "value": "attachment"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
